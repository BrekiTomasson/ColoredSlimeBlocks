{
  "schemaVersion": 1,
  "id": "${modid}",
  "version": "${mod_version}",

  "name": "Colored Slime Blocks",
  "description": "Dye your slime blocks!",
  "authors": ["Breki Tomasson"],
  "contact": {
    "homepage": "https://github.com/BrekiTomasson/ColoredSlimeBlocks",
    "sources": "https://github.com/BrekiTomasson/ColoredSlimeBlocks"
  },

  "license": "MIT",
  "icon": "assets/${modid}/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "${namespace}.coloredslime.ColoredSlime"
    ],
    "client": [
      "${namespace}.coloredslime.ColoredSlimeClient"
    ]
  },
  "mixins": [
    "coloredslime.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=${minimum_loader_version}",
    "fabric": "*",
    "minecraft": ">=${minimum_minecraft_version}"
  }
}
